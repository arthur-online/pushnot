<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "aginova.phonegap.app"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>Aginova App</name>
<icon src="icon.png" width="20" />
<gap:splash src="splash.png" />
    <preference name="fullscreen" value="true" />

    <description>
        Aginova Internals App 
    </description>
     <!--<gap:plugin name="org.li8.inappbrowser" version="0.1" />-->
  <gap:plugin name="com.adobe.plugins.pushplugin" />
<gap:plugin name="com.phonegap.plugin.statusbar" version="1.1.0" />
<preference name="StatusBarOverlaysWebView" value="false" />
<preference name="StatusBarBackgroundColor" value="#000000" />
<preference name="StatusBarStyle" value="lightcontent" />
    <author href="http://www.aginova.com" email="arthur@aginova.com">
        Arthur Coutinho
    </author>

</widget>
